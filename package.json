        "serve": "cross-env NODE_ENV=production ts-node server"
    },
    "dependencies": {
        "@mdi/font": "^5.9.55",
        "autoprefixer": "^10.2.5",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "postcss": "^8.2.14",
        "serve-static": "^1.14.1",
        "tailwindcss": "^2.1.2",
        "ts-node": "^9.1.1",
        "vue": "^3.0.5",
        "vue-router": "4"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/node": "^15.0.2",
        "@vitejs/plugin-vue": "^1.2.2",
        "@vue/compiler-sfc": "^3.0.5",
        "@vue/server-renderer": "^3.0.11",
        "eslint": "^7.26.0",
        "prettier": "^2.2.1",
        "sass": "^1.32.12",
        "typescript": "^4.1.3",
        "vite": "^2.2.3",
        "vue-tsc": "^0.0.24"
    }
